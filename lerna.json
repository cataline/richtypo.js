{
	"packages": ["packages/*", "example"],
	"version": "independent",
  "command": {
    "version": {
      "allowBranch": "master",
      "message": "chore: Publish :shipit: [skip ci]"
    },
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": [
        "package-lock.json",
        "*.md",
        "*.spec.js",
        "*.snap"
      ]
    }
  }
}
